   "source": [
    "def decorador(func):\n",
    "    def camuflador(*arg):\n",
    "        comida = func(*arg)\n",
    "        return \"Come \" + comida\n",
    "    return camuflador\n",
    "\n",
    "@decorador\n",
    "def comida(comida):\n",
    "    return comida\n",
    "\n",
    "comida(\"comida\")\n"
   ]
